[
  {
    "name":"exosphere-web-service",
    "image": "hugobho/exosphere-web-service",
    "cpu":100,
    "memory": 500,
    "essential":true,
    "portMappings": [
      {
        "hostPort": 80,
        "containerPort": 4001,
        "protocol": "tcp"
      },
      {
        "hostPort": 0,
        "containerPort": 3100,
        "protocol": "tcp"
      }
    ],
    "environment" : [
      { "name" : "EXOCOM_HOST",  "value" : "exocomm.us-west-1a.aws.megabyt.es."},
      { "name" : "EXOCOM_PORT",  "value" : "3100"},
      { "name" : "SERVICE_NAME", "value" : "web"}
    ]
  }
]
